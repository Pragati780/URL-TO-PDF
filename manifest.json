{
  "manifest_version": 3,
  "name": "URL to PDF",
  "version": "1.0",
  "description": "Convert a URL to PDF",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  }
}